{
  "seed": 42,
  "architecture": "recurrent_indexed",
  "dataset_name": "wikitext",
  "dataset_config_name": "wikitext-103-raw-v1",
  "tokenizer_name": "gpt2",
  "context_length": 128,
  "num_train_epochs": 1,
  "max_steps": 2,
  "per_device_train_batch_size": 2,
  "per_device_eval_batch_size": 2,
  "gradient_accumulation_steps": 1,
  "learning_rate": 0.0003,
  "warmup_ratio": 0.03,
  "weight_decay": 0.1,
  "adam_beta1": 0.9,
  "adam_beta2": 0.95,
  "adam_epsilon": 1e-8,
  "max_grad_norm": 1.0,
  "logging_steps": 1,
  "eval_steps": 2,
  "save_steps": 2,
  "save_total_limit": 1,
  "bf16": false,
  "fp16": false,
  "gradient_checkpointing": false,
  "dataloader_num_workers": 0,
  "report_to": "none",
  "run_name": "recurrent-indexed-tiny-smoke",
  "output_dir": "runs/recurrent_indexed_tiny_smoke",
  "overwrite_output_dir": true,
  "model": {
    "vocab_size": 50257,
    "n_positions": 128,
    "n_ctx": 128,
    "n_embd": 128,
    "n_layer": 4,
    "n_head": 4,
    "n_inner": 512,
    "activation_function": "gelu_new",
    "resid_pdrop": 0.0,
    "embd_pdrop": 0.0,
    "attn_pdrop": 0.0,
    "layer_norm_epsilon": 1e-5,
    "initializer_range": 0.02,
    "use_cache": false
  }
}
